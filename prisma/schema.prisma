generator client {
    provider = "prisma-client-py"
}

datasource db {
      provider = "postgresql"
      url      = env("DATABASE_URL")
}

model urls {
      id   String @id @default(uuid())
      original_url String @unique
      shorten_url String @unique
      no_of_visits Int @default(0)
      createdAt DateTime @default(now())
}